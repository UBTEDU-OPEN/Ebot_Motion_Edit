/************ QComboBox begin ************/
QComboBox#cBNation, QComboBox#cbxCountryCode
{
    border-left-width:0px;
    border-right-width: 1px;
    border-right-color: #070707;
    border-right-style: solid;
    border-top-width: 1px;
    border-top-color: #070707;
    border-top-style: solid;
    border-bottom-width: 1px;
    border-bottom-color: #070707;
    border-bottom-style: solid;
    background:#9f9f9f;
    font-size:14px;
    color:#060606;
}

QComboBox#cBNation QAbstractItemView, QComboBox#cbxCountryCode QAbstractItemView
{
    background: #9f9f9f;
    selection-background-color: #365370;
}

QComboBox QAbstractItemView::item
{
  min-height: 28px;
  max-height: 28px;
}

QComboBox#cBNation::drop-down, QComboBox#cbxCountryCode::drop-down
{
    subcontrol-origin: padding;
    subcontrol-position: top right;
    min-width: 16px;
    max-width: 16px;
    border-width: 0px;
}

QComboBox#cBNation::down-arrow, QComboBox#cbxCountryCode::down-arrow
{
    image: url(:/res/images/icon_choice.png);
}

