/* Style the tab using the tab sub-control. Note that
    it reads QTabBar _not_ QTabWidget */
QTabBar::tab
{
    border: 0px;
    font:16px;
    color: #d3d3d3;
    min-width: 140px;
    min-height: 25px;
    margin: 0px;
    padding: 2px;
}

QTabBar::tab:selected
{
    font:16px;
    color: #d3d3d3;
    margin: 0px;
    padding: 0px;
    border-bottom-width: 2px;
    border-bottom-style: solid;    
    border-bottom-color: #9f9f9f;
}

/************ tabWidgetLogin end ************/


